SELECT Famous_People.Name, Actors.Known_For AS "Known For", Actors.Awards_Won AS "Awards Won" FROM Famous_People
    JOIN Actors
    ON Famous_People.id = Actors.People_Id;
    
SELECT Famous_People.Name, Musicians.Famous_Song AS "Famous Song", Musicians.Awards_Won AS "Awards Won" FROM Famous_People
    JOIN Musicians
    ON Famous_People.id = Musicians.People_Id;

SELECT Famous_People.Name, Athletes.Sport, Athletes.Team, Athletes.Championships_Won AS "Championships Won" FROM Famous_People
    JOIN Athletes
    ON Famous_People.id = Athletes.People_Id;
    
SELECT A.Name AS Athlete, B.Name AS Teammate, Athletes.Team FROM Athletes 
    JOIN Famous_People A
    ON Athletes.People_Id = A.Id
    JOIN Famous_People B
    ON Athletes.Teammate_Id = B.Id
    GROUP BY Team;

SELECT Famous_People.Name, Film_Directors.Known_For AS "Known For", Film_Directors.Awards_Won AS "Awards Won"FROM Famous_People
    JOIN Film_Directors
    ON Famous_People.id = Film_Directors.People_Id
    GROUP BY Name;

SELECT A.Name AS Director, B.Name AS "Actor", Film_Directors.Known_For AS "Worked On" FROM Film_Directors
    JOIN Famous_People A
    ON Film_Directors.People_Id = A.Id
    JOIN Famous_People B
    ON Film_Directors.Actors_Worked_With_Id = B.Id;
