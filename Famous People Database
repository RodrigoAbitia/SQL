CREATE TABLE Famous_People (id INTEGER PRIMARY KEY AUTOINCREMENT,
    Name TEXT,
    Occupation TEXT);

CREATE TABLE Actors (id INTEGER PRIMARY KEY AUTOINCREMENT,
    People_Id INTEGER,
    Known_For TEXT,
    Awards_Won INTEGER);

CREATE TABLE Musicians (id INTEGER PRIMARY KEY AUTOINCREMENT,
    People_Id INTEGER,
    Famous_Song TEXT,
    Awards_Won INTEGER);
    
CREATE TABLE Athletes (id INTEGER PRIMARY KEY AUTOINCREMENT,
    People_Id INTEGER,
    Sport TEXT,
    Team TEXT,
    Championships_Won INTEGER,
    Teammate_Id INTEGER);

CREATE TABLE Film_Directors (id INTEGER PRIMARY KEY AUTOINCREMENT,
    People_Id INTEGER,
    Known_For TEXT,
    Awards_Won INTEGER,
    Actors_Worked_With_Id INTEGER
    );
    
