SELECT * FROM Letterboxd;

UPDATE Letterboxd SET Watch = "Watched" WHERE id = 14;
UPDATE Letterboxd SET Like = "Liked" WHERE id = 14;
UPDATE Letterboxd SET Rating = 4 WHERE id = 14;

SELECT * FROM Letterboxd;

