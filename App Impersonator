/* What does the app's SQL look like? */
CREATE TABLE Letterboxd (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    Film TEXT,
    Release_Date TEXT,
    Watch TEXT,
    Like TEXT,
    Rating INTEGER
    );
    
