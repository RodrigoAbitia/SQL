SELECT Player, Seasons_in_league,
    CASE
    WHEN Seasons_in_league = 0 Then "Rookie"
    WHEN Seasons_in_league = 1 Then "Sophmore"
    WHEN Seasons_in_league = 2 OR Seasons_in_league = 3 Then "Experienced"
    WHEN Seasons_in_league = 4 OR Seasons_in_league = 5 OR Seasons_in_league = 6  Then "Veteran"
    WHEN Seasons_in_league >= 7 Then "Seasoned Veteran"
    END AS "Experience Level"
FROM players
GROUP BY Player;

SELECT ROUND(AVG(Seasons_in_league)) AS Avg_Seasons_In_League_For_POTW FROM players;

SELECT Player, COUNT(Player) AS POTW_Times_Won FROM players GROUP BY Player;

SELECT Player, COUNT(Player) AS POTW_Times_Won FROM players GROUP BY Player HAVING POTW_Times_Won >= 3;

SELECT MAX(Position) AS Position_With_Most_POTW FROM players;

SELECT MIN(Position) AS Position_With_Least_POTW FROM players;
